<style>
  * {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
  }

  :root {
    --text: #2d2d2d;
    --text-light: #5a5a5a;
    --accent: #1a1a1a;
    --link: #4a4a4a;
    --border: rgba(208, 203, 192, 0.3);
    --timeline-line: rgba(45, 45, 45, 0.2);
  }

  body {
    background: linear-gradient(
      180deg,
      #87CEEB 0%,
      #B8D4E8 15%,
      #E8C4D8 35%,
      #F5B5A8 55%,
      #F4C8A0 75%,
      #5B7C9E 100%
    );
    min-height: 100vh;
    color: var(--text);
    font-family: 'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif;
    font-size: 16px;
    line-height: 1.6;
    -webkit-font-smoothing: antialiased;
  }

  .top-nav {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 24px;
    max-width: 900px;
    margin: 0 auto;
  }

  .back-link {
    color: var(--link);
    text-decoration: none;
    font-weight: 500;
    transition: color 0.2s ease;
  }

  .back-link:hover {
    color: var(--accent);
  }

  .nav-links {
    display: flex;
    gap: 24px;
  }

  .nav-links a {
    color: var(--link);
    text-decoration: none;
    font-weight: 500;
    transition: color 0.2s ease;
  }

  .nav-links a:hover {
    color: var(--accent);
  }

  main {
    max-width: 900px;
    margin: 0 auto;
    padding: 40px 24px 80px;
  }

  .intro {
    margin-bottom: 60px;
  }

  h1 {
    font-size: 36px;
    font-weight: 600;
    margin-bottom: 16px;
    color: var(--accent);
  }

  .subtitle {
    font-size: 18px;
    color: var(--text-light);
    line-height: 1.6;
  }

  h2 {
    font-size: 28px;
    font-weight: 600;
    margin-bottom: 32px;
    color: var(--accent);
  }

  .timeline {
    position: relative;
    padding-left: 50px;
  }

  .timeline::before {
    content: '';
    position: absolute;
    left: 16px;
    top: 0;
    bottom: 0;
    width: 2px;
    background: var(--timeline-line);
  }

  .timeline-item {
    position: relative;
    margin-bottom: 40px;
  }

  .timeline-marker {
    position: absolute;
    left: -33px;
    top: 6px;
    width: 32px;
    height: 32px;
    border-radius: 50%;
    background: white;
    border: 2px solid var(--border);
    box-shadow: 0 0 0 4px rgba(45, 45, 45, 0.05);
    display: flex;
    align-items: center;
    justify-content: center;
    overflow: hidden;
  }

  .timeline-logo {
    width: 20px;
    height: 20px;
    object-fit: contain;
  }

  .timeline-icon {
    display: flex;
    align-items: center;
    justify-content: center;
    color: var(--accent);
  }

  .timeline-icon svg {
    width: 16px;
    height: 16px;
  }

  .timeline-content {
    background: rgba(255, 255, 255, 0.6);
    backdrop-filter: blur(10px);
    border: 1px solid var(--border);
    border-radius: 8px;
    padding: 20px 24px;
  }

  .timeline-header {
    display: flex;
    justify-content: space-between;
    align-items: baseline;
    margin-bottom: 4px;
    gap: 16px;
  }

  .timeline-content h3 {
    font-size: 18px;
    font-weight: 600;
    color: var(--accent);
  }

  .timeline-date {
    font-size: 14px;
    color: var(--text-light);
    white-space: nowrap;
  }

  .timeline-company {
    font-size: 16px;
    color: var(--link);
    font-weight: 500;
    margin-bottom: 8px;
  }

  .timeline-description {
    font-size: 15px;
    color: var(--text-light);
    margin-top: 8px;
  }

  .timeline-details {
    margin-top: 12px;
    padding-left: 20px;
  }

  .timeline-details li {
    font-size: 15px;
    color: var(--text);
    margin-bottom: 6px;
    line-height: 1.5;
  }

  .skills {
    margin-top: 80px;
  }

  .skills-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
    gap: 24px;
  }

  .skill-category {
    background: rgba(255, 255, 255, 0.6);
    backdrop-filter: blur(10px);
    border: 1px solid var(--border);
    border-radius: 8px;
    padding: 20px;
  }

  .skill-category h3 {
    font-size: 16px;
    font-weight: 600;
    color: var(--accent);
    margin-bottom: 8px;
  }

  .skill-category p {
    font-size: 14px;
    color: var(--text-light);
    line-height: 1.5;
  }

  @media (max-width: 640px) {
    .timeline {
      padding-left: 42px;
    }

    .timeline::before {
      left: 12px;
    }

    .timeline-marker {
      left: -27px;
      width: 28px;
      height: 28px;
    }

    .timeline-icon svg {
      width: 14px;
      height: 14px;
    }

    .timeline-header {
      flex-direction: column;
      align-items: flex-start;
      gap: 4px;
    }

    h1 {
      font-size: 28px;
    }

    h2 {
      font-size: 24px;
    }
  }
</style>